<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teghpp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="teghpp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.teghpp.Hora"></activity>
        <activity
            android:name="com.teghpp.PerdidaActivity"
            android:parentActivityName="com.teghpp.MenuActivity"
            android:screenOrientation="portrait">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teghpp.MenuActivity" />
        </activity>
        <activity
            android:name="com.teghpp.PresionActivity"
            android:parentActivityName="com.teghpp.MenuActivity"
            android:screenOrientation="portrait">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teghpp.MainActivity" />
        </activity>
        <activity
            android:name="com.teghpp.MenuActivity"
            android:parentActivityName="com.teghpp.MainActivity"
            android:screenOrientation="portrait">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teghpp.MainActivity" />
        </activity>
        <activity
            android:name="com.teghpp.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.teghpp.Registro"
            android:parentActivityName="com.teghpp.MainActivity"
            android:screenOrientation="portrait">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.teghpp.MainActivity" />
        </activity>
    </application>

</manifest>